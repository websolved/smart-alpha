				<!--<li class="breadcrumb-item"><a href="javascript:;">Library</a></li>
				<li class="breadcrumb-item active"><a href="javascript:;">Data</a></li>-->
			</ol>
			 <!-- end breadcrumb -->
			<!-- begin page-header -->
			<h1 class="page-header">New Registrations <small></small></h1>
			<!-- end page-header -->
			<!-- begin panel -->
            <div class="col-xl-8">
                <div class="panel panel-inverse">
                    <div class="panel-heading">
                        <h4 class="panel-title">New Entries</h4>
                    </div>
                    <div class="panel-body">
                        <div class="table-responsive">
                            <table class="table table-striped m-b-0">
                                <thead>
                                    <tr>
                                        <th>#</th>
                                        <th>Name</th>
                                        <th>Phone</th>
                                        <th>Interested In</th>
                                        <th>Submission Time</th>
                                        <th width="1%"></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% var i=1 %> 
                                    <% register.forEach(register => { %>
                                    <tr>
                                        <td>
                                            <%= i++ %> 
                                            <!--<img src="/admin/img/user/user-1.jpg" class="img-rounded height-30" />-->
                                        </td>
                                        <td><%= register.firstname %> <%= register.lastname %></td>
                                        <td><%= register.phone %></td>
										<% var product = null %>
										<% if(register.interest1 != null) { %>
										<% product = register.interest1 %>
										<% } %>
										<% if(register.interest2 != null) { %>
										<% if(product == null) {product = register.interest2 %> 
										<% } else { %>
										<% product = product + ", " + register.interest2 %>
										<% } %>
										<% } %>
										<% if(register.interest3 != null) { %>
										<% if(product == null) { product = register.interest3 %>
										<% }else { %>
										<% product = product + ", " + register.interest3 } %>
										<% } %>
										<% if (product == null) { %>
                                        <td style="text-transform: uppercase;">N/A</td>
										<% } else { %>
											<td style="text-transform: uppercase;"><%= product %></td>
											<% } %>
											<% var time = register.time.getHours() %>
											<% time = time + ":" + register.time.getMinutes() %>
                                        <td><%= register.time.toDateString() %> <%= time %></td>
                                        <td class="with-btn m-r-0" nowrap>
                                            <a href="#" class="btn btn-sm btn-primary width-80">Approve</a>
                                            <!--<a href="#" class="btn btn-sm btn-white width-60">Delete</a>-->
                                        </td>
                                    </tr>
                                    <% }) %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
			 <!-- end panel -->
